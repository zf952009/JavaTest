<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
    <package name="Demo2Action" namespace="/demo" extends="struts-default">
        <!--找不到action会使用Demo2Action作为默认action处理请求-->
        <default-action-ref name="Demo2Action"></default-action-ref>
        <!--
            method属性：默认值为execute
            result的默认name属性：success
            clas默认属性：com.opensymphony.xwork2.ActionSupport
        -->
        <action name="Demo2Action">
            <result>/index.jsp</result>
        </action>
    </package>
</struts>
